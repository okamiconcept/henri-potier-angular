{
  "name": "henri-potier-angular",
  "homepage": "https://okamiconcept.github.io/henri-potier-angular",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:stats": "ng build --prod --statsJson --sourceMap",
    "build:analyze": "yarn build:stats && yarn analyze",
    "build:stats:report": "yarn build:stats && yarn bundle-report",
    "bundle-report": "webpack-bundle-analyzer dist/henri-potier-angular/stats-es2015.json",
    "analyze": "source-map-explorer 'dist/henri-potier-angular/*-es2015.*.js'",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "predeploy": "yarn build -- --base-href=/henri-potier-angular/",
    "deploy": "gh-pages -d dist/henri-potier-angular"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.9",
    "@angular/common": "~8.2.9",
    "@angular/compiler": "~8.2.9",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.2.9",
    "@angular/platform-browser": "~8.2.9",
    "@angular/platform-browser-dynamic": "~8.2.9",
    "@angular/router": "~8.2.9",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1",
    "bulma": "^0.7.5",
    "bulma-badge": "^3.0.1",
    "@fortawesome/fontawesome-free": "^5.11.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.9",
    "@angular/cli": "~8.3.9",
    "@angular/compiler-cli": "~8.2.9",
    "@angular/language-service": "~8.2.9",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "webpack-bundle-analyzer": "^3.5.2",
    "source-map-explorer": "^2.1.0",
    "gh-pages": "^2.1.1"
  }
}
